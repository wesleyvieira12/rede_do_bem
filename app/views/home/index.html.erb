    <!-- contact section starts here -->
    <section class="service text-center" id="contact">
        <div class="container">
            <div class="row">
                
                    <div class="contact-heading text-center">
                        <h2>Contato</h2>
                    </div>

                    <div class="col-md-5 contact-info text-left">
                        <h3>Informações de contato</h3>
                        <div class="info-detail">
                            <ul><li><i class="fa fa-map-marker"></i><span>Endereço:</span> 123 Some Street , London, UK, CP 123</li></ul>
                            <ul><li><i class="fa fa-phone"></i><span>Telefone:</span> (032) 987-1235</li></ul>
                            <ul><li><i class="fa fa-fax"></i><span>Fax:</span> (123) 984-1234</li></ul>
                            <ul><li><i class="fa fa-envelope"></i><span>Email:</span> <a class="__cf_email__" href="/cdn-cgi/l/email-protection" data-cfemail="7d14131b123d19121e09120f531e1210">[email&#160;protected]</a><script data-cfhash='f9e31' type="text/javascript">/* <![CDATA[ */!function(t,e,r,n,c,a,p){try{t=document.currentScript||function(){for(t=document.getElementsByTagName('script'),e=t.length;e--;)if(t[e].getAttribute('data-cfhash'))return t[e]}();if(t&&(c=t.previousSibling)){p=t.parentNode;if(a=c.getAttribute('data-cfemail')){for(e='',r='0x'+a.substr(0,2)|0,n=2;a.length-n;n+=2)e+='%'+('0'+('0x'+a.substr(n,2)^r).toString(16)).slice(-2);p.replaceChild(document.createTextNode(decodeURIComponent(e)),c)}p.removeChild(t)}}catch(u){}}()/* ]]> */</script></li></ul>
                        </div>

                        <%= bootstrap_flash%>
                    </div>

                    <% contato = Contato.new %>
                    
                    <%= form_for(contato) do |f| %>

                    <% if contato.errors.any? %>
                            <div id="error_explanation">
                              <h2><%= pluralize(contato.errors.count, "error") %> prohibited this contato from being saved:</h2>

                              <ul>
                              <% contato.errors.full_messages.each do |message| %>
                                <li><%= message %></li>
                              <% end %>
                              </ul>
                            </div>
                    <% end %>

                    <div class="col-md-6 col-md-offset-1 contact-form">

                        <h3>Digite sua mensagem</h3>

                        <form class="form">

                            <div class="name">
                                
                                <%= f.text_field :name, class: "form-control", placeholder: "Nome" %>
                            </div>

                            <div class="email">
                                
                                <%= f.text_field :email, class: "form-control", placeholder: "Email"  %>
                            </div>

                            <div class="phone">
                                
                                <%= f.text_field :phone, class: "form-control", placeholder: "Telefone"  %>
                            </div>

                            <div class="message">
                                
                                <%= f.text_area :message, class: "form-control", placeholder: "Mensagem"  %>
                            </div>

                            <div class="actions">
                                <%= f.submit 'Enviar', :class => 'submit-btn' %>
                            </div><br>

                        </form>
                    </div>

                    <% end %>

                </div>
            
        </div>
    </section><!-- end of contact section -->